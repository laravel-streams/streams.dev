{
    "$schema": "./../bootstrap/cache/schema/stream.schema.json",
    "name": "Todos",
    "handle": "todos",
    "description": "A todos testing stream.",
    "config": {
        "source": {
            "type": "self"
        }
    },
    "fields": [
        {
            "handle": "id",
            "type": "uuid",
            "unique": true,
            "required": true,
            "config": {
                "default": true
            }
        },
        {
            "handle": "title",
            "type": "string",
            "required": true
        },
        {
            "handle": "complete",
            "type": "boolean",
            "config": {
                "default": false
            }
        }
    ],
    "data": {
        "655de760-6ba0-3f4f-ad0c-8051588ad6e2": {
            "title": "The first todo!",
            "complete": false
        }
    }
}
