{
    "name": "SDK Documentation",
    "extends": "docs",
    "url_prefix": "docs/sdk",
    "path_prefix": "vendor/streams/sdk/docs",
    "routes": [
        {
            "handle": "view",
            "uri": "docs/sdk/{id}",
            "view": "doc",
            "stream": "docs_sdk"
        }
    ],
    "description": "Documentation for all things contained in the SDK package.",
    "project": "streams-sdk",
    "github_docs": "https://github.com/laravel-streams/streams-sdk/blob/1.0/docs",
    "ui": {
        "cp": {
            "section": {
                "parent": "docs",
                "title": "SDK",
                "stream": "docs_sdk",
                "buttons": [
                    {
                        "handle": "improve",
                        "text": "View Docs",
                        "attributes": {
                            "target": "_blank",
                            "url": "/docs/sdk/introduction"
                        }
                    }
                ]
            }
        }
    }
}
