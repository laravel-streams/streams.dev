{
    "name": "Streams API",
    "extends": "docs",
    "url_prefix": "docs/api",
    "path_prefix": "vendor/streams/api/docs",
    "routes": [
        {
            "handle": "view",
            "uri": "docs/api/{id}",
            "view": "doc",
            "stream": "docs_api"
        }
    ],
    "description": "Documentation for all things contained in the API package.",
    "project": "streams/api",
    "github_docs": "https://github.com/laravel-streams/streams-api/blob/1.0/docs",
    "ui": {
        "admin": {
            "navigation": [
                {
                    "sort_order": 2,
                    "text": "Docs API",
                    "url": "\/admin\/docs_api",
                    "component": "anchor"
                }
            ]
        }
    }
}
