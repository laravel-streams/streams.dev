{
    "name": "Streams UI",
    "extends": "docs",
    "url_prefix": "docs/ui",
    "path_prefix": "vendor/streams/ui/docs",
    "routes": [
        {
            "handle": "view",
            "uri": "docs/ui/{id}",
            "view": "doc",
            "stream": "docs_ui"
        }
    ],
    "description": "Documentation for all things contained in the UI package.",
    "project": "streams/ui",
    "github_docs": "https://github.com/laravel-streams/streams-ui/blob/1.0/docs",
    "ui": {
        "admin": {
            "navigation": [
                {
                    "sort_order": 2,
                    "text": "Docs UI",
                    "url": "\/admin\/docs_ui",
                    "component": "anchor"
                }
            ]
        }
    }
}
