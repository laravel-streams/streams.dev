{
    "name": "Packages",
    "description": "Packages for Laravel Streams.",
    "config": {
        "source": {
            "type": "self"
        }
    },
    "routes": [
        {
            "handle": "view",
            "uri": "/package/{id}",
            "parse": true,
            "view": "package"
        }
    ],
    "fields": [
        {
            "handle": "id",
            "type": "uuid",
            "required": true,
            "unique": true,
            "config": {
                "default": true
            }
        },
        {
            "handle": "name",
            "type": "string"
        },
        {
            "handle": "type",
            "type": "string"
        },
        {
            "handle": "enabled",
            "type": "boolean",
            "config": {
                "default": false
            }
        },
        {
            "handle": "composer",
            "type": "object",
            "input": {
                "type": "editor",
                "language": "json"
            }
        }
    ],
    "ui": {
        "cp": {
            "section": {
                "buttons": [
                    {
                        "handle": "create"
                    }
                ]
            }
        },
        "tables": [
            {
                "handle": "default",
                "filters": [
                    {
                        "handle": "name"
                    },
                    {
                        "handle": "type"
                    }
                ],
                "columns": [
                    {
                        "handle": "name"
                    },
                    {
                        "handle": "type"
                    },
                    {
                        "handle": "enabled"
                    }
                ],
                "buttons": [
                    {
                        "handle": "edit"
                    }
                ]
            }
        ],
        "forms": [
            {
                "handle": "default",
                "buttons": [
                    {
                        "handle": "cancel",
                        "class": "-secondary",
                        "attributes": {
                            "href": "{cp.section.url}"
                        }
                    }
                ]
            }
        ]
    },
    "data": {
        "598b243b-36c9-4e0b-b7a1-47edbb6376e9": {
