                    {
                        "type": "path",
                        "url": "../streams-api",
                        "options": { "symlink": true }
                    },
                    {
                        "type": "path",
                        "url": "../streams-sdk",
                        "options": { "symlink": true }
                    },
                    {
                        "type": "path",
                        "url": "../streams-ui",
                        "options": { "symlink": true }
                    }
                ],
                "autoload": { "psr-4": {} }
            }
        },
