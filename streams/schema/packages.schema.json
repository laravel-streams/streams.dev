{
    "name": "Packages",
    "type": "object",
    "properties": {
        "id": {
            "title": "Id",
            "type": "string",
            "pattern": "^[a-z0-9]+(?:[-_][a-z0-9]+)*$"
        },
        "name": {
            "title": "Name",
            "type": "string"
        },
        "docs": {
            "title": "Docs",
            "type": "string",
            "pattern": "https?:\\/\\/(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)"
        }
    }
}